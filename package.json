{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "add-expose": "node scripts/typeorm-post-process.js",
    "import-sql": "psql -h localhost -p 5432 -U admin -d hangma -f src/database/bootstrap_table.sql",
    "generate-entity": "typeorm-model-generator -h 127.0.0.1 -d hangma -u admin -x 1234 -e postgres -o ./src/entities --noConfig true --ce pascal --cp camel",
    "rename-entity": "node scripts/entity-file-rename.js && scripts/singularize-filename.js && node scripts/singularize-filename.js && node scripts/singularize-classname.js",
    "generate-and-process-entity": "yarn generate-entity && yarn add-expose && yarn rename-entity"
  },
  "dependencies": {
    "@nestjs/common": "10.3.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "10.3.2",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "3.9.8",
    "pg": "^8.12.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.1.0",
    "@types/supertest": "2.0.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "pluralize": "^8.0.0",
    "prettier": "3.2.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
